<?xml version='1.0' encoding='utf-8'?>
<widget android-version-code="2" id="io.acgt.hockey" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name short="AKL Hockey">Auckland Hockey</name>
  <description>Get Auckland's ice hockey scene in your pocket!</description>
  <author>Thomas Paulin</author>
  <content src="index.html"/>
  <access origin="https://snc-api-staging.herokuapp.com/*"/>
  <access origin="https://snc-api.herokuapp.com/*"/>
  <access origin="*://*.cloudinary.com/inspgdgt/*"/>
  <allow-navigation href="http://ionic.local/*"/>
  <allow-navigation href="http://www.aucklandsnchockey.com/*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="LogLevel" value="WARN"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="webviewbounce" value="false"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <icon density="ldpi" src="resources/android/icon/ldpi-icon.png"/>
    <icon density="mdpi" src="resources/android/icon/mdpi-icon.png"/>
    <icon density="hdpi" src="resources/android/icon/hdpi-icon.png"/>
    <icon density="xhdpi" src="resources/android/icon/xhdpi-icon.png"/>
    <icon density="xxhdpi" src="resources/android/icon/xxhdpi-icon.png"/>
    <icon density="xxxhdpi" src="resources/android/icon/xxxhdpi-icon.png"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
    <icon height="57" src="resources/ios/icon/icon.png" width="57"/>
    <icon height="114" src="resources/ios/icon/icon@2x.png" width="114"/>
    <icon height="40" src="resources/ios/icon/icon-40.png" width="40"/>
    <icon height="80" src="resources/ios/icon/icon-40@2x.png" width="80"/>
    <icon height="120" src="resources/ios/icon/icon-40@3x.png" width="120"/>
    <icon height="50" src="resources/ios/icon/icon-50.png" width="50"/>
    <icon height="100" src="resources/ios/icon/icon-50@2x.png" width="100"/>
    <icon height="60" src="resources/ios/icon/icon-60.png" width="60"/>
    <icon height="120" src="resources/ios/icon/icon-60@2x.png" width="120"/>
    <icon height="180" src="resources/ios/icon/icon-60@3x.png" width="180"/>
    <icon height="72" src="resources/ios/icon/icon-72.png" width="72"/>
    <icon height="144" src="resources/ios/icon/icon-72@2x.png" width="144"/>
    <icon height="76" src="resources/ios/icon/icon-76.png" width="76"/>
    <icon height="152" src="resources/ios/icon/icon-76@2x.png" width="152"/>
    <icon height="167" src="resources/ios/icon/icon-83.5@2x.png" width="167"/>
    <icon height="29" src="resources/ios/icon/icon-small.png" width="29"/>
    <icon height="58" src="resources/ios/icon/icon-small@2x.png" width="58"/>
    <icon height="87" src="resources/ios/icon/icon-small@3x.png" width="87"/>
  </platform>
  <engine name="android" spec="6.3.0"/>
  <plugin name="ionic-plugin-keyboard" spec="^2.2.1"/>
  <plugin name="cordova-plugin-whitelist" spec="^1.3.1"/>
  <plugin name="cordova-plugin-console" spec="^1.0.5"/>
  <plugin name="cordova-plugin-statusbar" spec="^2.2.2"/>
  <plugin name="cordova-plugin-device" spec="^1.1.4"/>
  <plugin name="cordova-plugin-splashscreen" spec="^4.0.3"/>
</widget>
